@model TestsSystem.Web.Models.UI.PartialNotification

<div class="@Model.AlertType text-center mt-2 alert alert-dismissible fade show" role="alert">
    <strong>@Model.SessionUserName!</strong> @Model.NotifyMessage.
    <button class="close"
            type="button"
            data-dismiss="alert"
            aria-label="Close">
        <span aria-hidden="true">×</span>
    </button>
</div>
